#include <stdio.h>
 int sumofarray(int a[], int n)
 {
 	int min,max,i;
 	min=max=a[0];
    for(i=1; i<n; i++)
    {
         if(min>a[i])
		  min=a[i];   
		   if(max<a[i])
		    max=a[i];       
    }
    
    printf("\n%d",min);
    printf("\n%d",max);
 }
int main()
{
    int a[1000],i,n,sum;
    scanf("%d", &n);
    for(i=0; i<n; i++)
    {
        scanf("%d",&a[i]);
    }
    sumofarray(a,n);
}
